    "proposito.html": {
      "settings": {
        "title": "Mision, vision y objetivos",
        "meta_descr": "Mision, vision y objetivos de Union Pro Animales Abandonados "
      },
      "components": [
        {
          "_styles": {
            ".mbr-section-title, .mbr-section-subtitle": {
              "text-align": "center"
            }
          },
          "_tags": [
            "Theme"
